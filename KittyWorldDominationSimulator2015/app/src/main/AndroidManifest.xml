<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loan.kittyworlddominationsimulator2015" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".opening_Screen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Continue"
            android:label="@string/title_activity_continue" >
        </activity>
        <activity
            android:name=".newGame"
            android:label="@string/title_activity_new_game" >
        </activity>
        <activity
            android:name=".credits"
            android:label="@string/title_activity_credits" >
        </activity>
        <activity
            android:name=".options"
            android:label="@string/title_activity_options" >
        </activity>
    </application>

</manifest>
